    <nav class="navbar navbar-inverse bg-inverse">
        <div class="container-fluid">
            <form class="navbar-form navbar-right" autocomplete="off" id="login">
                <div class="form-group">
                    <input type="text" id="user" class="form-control inverse" placeholder="User">
                    <input type="text" id="password" class="form-control inverse" placeholder="Password">
