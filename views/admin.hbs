    <div id="alertainer">
    </div>
    <div>
      <ul id="messages-area"></ul>
        <form id="form" onsubmit="return false;">
          <div class="form-group">
          <input class="form-control" id="new-message" type="text" />
          </div>
        <button class="btn" onclick="sendFunction()">Send</button>
        </form>
    </div>
<script src="../javascripts/ui.js"></script>
